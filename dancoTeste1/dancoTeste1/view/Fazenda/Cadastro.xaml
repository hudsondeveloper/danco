<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dancoTeste1.view.Fazenda.Cadastro"
			 Title="Cadastrar Fazenda"
			 BackgroundColor="#b9a375">
    <ContentPage.Content>
		<StackLayout >
			<Label Text="Nome" TextColor="White" />
			<Entry x:Name="etNome" Placeholder="Nome" BackgroundColor="#b9a375" ></Entry>

			<Label Text="Fazenda" TextColor="White"/>
			<Entry x:Name="etFazenda" Placeholder="codigo da Fazenda"  BackgroundColor="#b9a375"></Entry>

			<Button x:Name="btnCadastro" Text="Cadastrar" HorizontalOptions="Center" BackgroundColor="#a7c45a" Clicked="cadastro_clicked"/>
		</StackLayout>
    </ContentPage.Content>
</ContentPage>