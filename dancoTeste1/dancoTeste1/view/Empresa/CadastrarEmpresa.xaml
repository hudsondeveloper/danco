<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="dancoTeste1.view.Empresa.CadastrarEmpresa">
	<ContentPage.Content>
		<StackLayout BackgroundColor="#F7BE81">
				<Label Text="Nome" TextColor="White" HorizontalOptions="Center"/>
				<Entry x:Name="etNome" Placeholder="Nome" BackgroundColor="White" Margin ="20,0,20,0"></Entry>

				<Label Text="E-mail" TextColor="White" HorizontalOptions="Center"/>
				<Entry x:Name="etEmail" Placeholder="E-mail"  BackgroundColor="White"  Margin ="20,0,20,0"></Entry>

				<Button x:Name="btnCadastro" Text="Cadastrar" HorizontalOptions="Center" Clicked="cadastro_clicked" BorderRadius="30"/>
			</StackLayout>
    </ContentPage.Content>
</ContentPage>